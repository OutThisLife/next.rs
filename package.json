{
  "name": "rust",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:OutThisLife/next.rs.git",
  "author": "Talasan Nicholson <talasan.nicholson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build:client && cd server && cargo run",
    "build": "yarn build:server && yarn build:client",
    "build:client": "parcel build static/index.html",
    "build:server": "cd server && cargo build",
    "test:server": "cd server && cargo watch -x \"test\"",
    "dev": "parcel static/index.html -p 3000 --no-cache",
    "test:dev": "cd client && cargo watch -x \"test\""
  },
  "devDependencies": {
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-cargo-web": "^0.3.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.0.0-beta.53"
  }
}
